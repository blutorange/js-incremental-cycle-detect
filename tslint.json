{
    "$schema": "http://json.schemastore.org/tslint",   
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
    },
    "rules": {
        "arrow-parens": {
            "options": "ban-single-arg-parens"
        },
        "array-type": {
            "options": "array"
        },
        "interface-name": {
            "options": "never-prefix"
        },
        "strictNullChecks": true,
        "one-line": false,
        "forin": false,
        "use-isnan": true,
        "trailing-comma": true,
        "semicolon": true,
        "radix": true,
        "no-void-expression": true,
        "no-unsafe-any": true,
        "quotemark": {
            "options": "double",
        },
        "no-unused-variable": true,
        "no-unsafe-finally": true,
        "prefer-const": true
    },
    "rulesDirectory": []
}